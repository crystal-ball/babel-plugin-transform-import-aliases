{
  "name": "babel-plugin-transform-import-aliases",
  "version": "1.2.0",
  "description": "Transforms import aliases into resolveable paths",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/babel-plugin-transform-import-aliases.git"
  },
  "keywords": [
    "Babel",
    "transform",
    "import",
    "alias"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty --ext .js,.ts src",
    "test:unit": "jest src",
    "format": "prettier --write '*.{js,json,md}' '**/*.{js,json,md,mdx,scss}' !CHANGELOG.md"
  },
  "peerDependencies": {
    "@babel/types": "7"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@crystal-ball/commitizen-base": "2.9.0",
    "@crystal-ball/prettier-base": "2.4.0",
    "@crystal-ball/semantic-release-base": "2.24.0",
    "@types/jest": "26.0.15",
    "eslint-config-eloquence": "16.8.0",
    "husky": "4.3.0",
    "jest": "26.6.3"
  },
  "prettier": "@crystal-ball/prettier-base",
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "release": {
    "extends": [
      "@crystal-ball/semantic-release-base"
    ]
  },
  "renovate": {
    "extends": [
      "github>crystal-ball/renovate-base:library"
    ]
  }
}
